    "jsdom": "^8.1.0",
    "mocha": "^10.1.0",
    "node-sass": "^4.13.1",
    "react-addons-test-utils": "^0.14.7",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
